
public class Declaraciones_Operadores {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 5;
		int b;
		b = 7;
		int c = b + a;
		System.out.println("c: " + c);
		System.out.println("c++ " + c++);
		System.out.println("++c " + ++c);
		System.out.println(c+=6);
		int d = 4;
		int e = 10;
		double f = e/d;
		System.out.println(f);
	}

}
